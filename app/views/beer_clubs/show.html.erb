<p id="notice"><%= notice %></p>

<p>
<h1><strong> <%= @beer_club.name %> </strong></h1>
</p>

<p>
  <strong>Founded:</strong>
  <%= @beer_club.founded %>
</p>

<p>
  <strong>City:</strong>
  <%= @beer_club.city %>
</p>

<br>
<br>

<p>
<h2> Members: </h2>
    <% @beer_club.users.each do |member| %>
      <tr>
        <td><%= member.username %></td>
        <td><%= link_to 'Show', member %></td>
      </tr>
    <% end %>
</p>

<%= link_to 'Edit', edit_beer_club_path(@beer_club) %> |
